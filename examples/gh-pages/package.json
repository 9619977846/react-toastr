    "node-sass": "^3.1.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.7",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.2",
    "webpack": "^1.9.8",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "animate.css": "^3.2.5",
    "bootstrap-sass": "^3.3.4",
    "classnames": "^2.1.1",
